{
   "name": "@muzikanto/observable",
   "version": "3.2.0",
   "description": "The state manager",
   "license": "MIT",
   "maintainers": [
      {
         "name": "Maxim Schiriy"
      }
   ],
   "main": "./index.js",
   "typings": "./index.d.ts",
   "homepage": "https://github.com/Muzikanto/observable",
   "keywords": [
      "react",
      "observable",
      "store",
      "state-manager"
   ],
   "repository": {
      "url": "https://github.com/Muzikanto/observable"
   },
   "bugs": {
      "url": "https://github.com/Muzikanto/observable/issues"
   },
   "scripts": {
      "clean": "rm -rf *.ts && rm -rf *.tsx && rm -rf *.js",
      "build": "tsc -b",
      "linter": "tslint -c tslint.json 'src/**/*.ts' --fix",
      "prepublish": "npm run build && npm run test",
      "postpublish": "npm run clean",
      "test": "jest",
      "test:info": "jest --coverage",
      "pretty": "pretty-quick --staged"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run clean && npm run pretty",
         "pre-push": "npm test",
         "...": "..."
      }
   },
   "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "verbose": true
   },
   "peerDependencies": {
      "react": "16.13.1",
      "react-dom": "16.13.1",
      "@types/react": "16.9.35",
      "@types/react-dom": "16.9.8"
   },
   "dependencies": {},
   "devDependencies": {
      "@types/jest": "25.2.3",
      "@types/node": "14.0.6",
      "install-peers": "1.0.3",
      "jest": "26.0.1",
      "ts-jest": "26.0.0",
      "tslint": "5.20.1",
      "typescript": "3.9.3",
      "husky": "4.2.5",
      "prettier": "2.0.5",
      "pretty-quick": "2.0.1"
   }
}
