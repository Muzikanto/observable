{
   "name": "@muzikanto/observable",
   "version": "2.0.4",
   "description": "The state manager",
   "license": "MIT",
   "maintainers": [
      {
         "name": "Maxim Schiriy"
      }
   ],
   "main": "./index.js",
   "typings": "./index.d.ts",
   "homepage": "https://github.com/Muzikanto/observable",
   "keywords": [
      "react",
      "observable",
      "form",
      "store",
      "state-manager"
   ],
   "repository": {
      "url": "https://github.com/Muzikanto/observable"
   },
   "bugs": {
      "url": "https://github.com/Muzikanto/observable/issues"
   },
   "scripts": {
      "clean": "rm -rf *.ts && rm -rf *.tsx && rm -rf *.js",
      "build": "tsc -b",
      "linter": "tslint -c tslint.json 'src/**/*.ts' --fix",
      "prepublish": "npm run build && npm run test",
      "postpublish": "npm run clean",
      "test": "jest",
      "test:info": "jest --coverage",
      "pretty": "pretty-quick --staged"
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run clean && npm run pretty",
         "pre-push": "npm test",
         "...": "..."
      }
   },
   "jest": {
      "preset": "ts-jest",
      "testEnvironment": "node",
      "verbose": true
   },
   "peerDependencies": {
      "react": "16.12.0",
      "react-dom": "16.12.0",
      "@types/react": "16.9",
      "@types/react-dom": "16.9",
      "@types/yup": "0.26.33"
   },
   "dependencies": {},
   "devDependencies": {
      "@types/jest": "25.1.4",
      "@types/node": "13.9.5",
      "install-peers": "1.0.3",
      "jest": "25.2.6",
      "ts-jest": "25.2.1",
      "tslint": "5.20.1",
      "typescript": "3.6",
      "husky": "3.0.9",
      "prettier": "1.18.2",
      "pretty-quick": "1.11.1"
   }
}
